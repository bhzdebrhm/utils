    },
    {
      "path": "dist/utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.191",
    "husky": "^8.0.2",
    "size-limit": "^8.1.0",
